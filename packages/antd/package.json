{
  "name": "@chocolateloverraj/react-json-input-antd",
  "description": "Ant design style react json input.",
  "version": "0.1.0",
  "author": {
    "name": "Rajas Paranjpe",
    "url": "https://github.com/ChocolateLoverRaj"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChocolateLoverRaj/react-json-input/issues"
  },
  "repository": {
    "url": "https://github.com/ChocolateLoverRaj/react-json-input",
    "directory": "packages/antd"
  },
  "homepage": "https://react-json-input.netlify.app/",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "build:empty": "rmdir-cli dist",
    "build:js": "babel --extensions \".ts,.tsx\" lib --out-dir dist --ignore \"**/*.test.tsx\",\"lib/scss.d.ts\"",
    "build:js:watch": "babel --extensions \".ts,.tsx\" lib --out-dir dist --ignore \"**/*.test.tsx\",\"lib/scss.d.ts\" -w",
    "build:dts": "tsc --project build-tsconfig.json",
    "build": "npm run build:empty && npm run build:js && npm run build:dts",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-react-require": "^3.1.3",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "rmdir-cli": "^2.0.6",
    "ts-standard": "^10.0.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "@chocolateloverraj/react-json-input": "^2.0.2"
  }
}